{
  "name": "vam-online",
  "version": "0.0.1",
  "description": "Electron implementation of VAM.",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server",
    "lint": "./node_modules/.bin/eslint ./",
    "test": "./node_modules/.bin/jest",
    "electron": "./node_modules/.bin/electron main.js",
    "build": "./node_modules/.bin/webpack -p"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/jestsetup.js",
      "(.test)\\.(ts|tsx|js)$"
    ],
    "collectCoverage": true,
    "transform": {
      ".*": "<rootDir>/node_modules/jest-babel-preprocessor/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "verbose": true,
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".+\\.(css|scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackiedrew/vam-online.git"
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "Mackie Drew",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/mackiedrew/vam-online/issues"
  },
  "homepage": "https://valence.audio/",
  "dependencies": {
    "electron": "1.6.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "codecov": "2.1.0",
    "css-loader": "0.28.0",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "electron-reload": "^1.1.0",
    "enzyme": "2.8.0",
    "enzyme-to-json": "1.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "jest-babel-preprocessor": "0.3.0",
    "jest-cli": "19.0.2",
    "jest-css-modules": "^1.1.0",
    "json-loader": "0.5.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-svg-loader": "1.1.1",
    "sass-loader": "6.0.3",
    "sinon": "2.1.0",
    "style-loader": "0.16.1",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  }
}
