{
  "name": "vam-online",
  "version": "0.0.1",
  "author": "Valence Audio <admin@valence.audio> (http://valence.audio/)",
  "contributors": [
    "Mackie Drew <mackiedrew@gmail.com> (http://mackiedrew.com/)"
  ],
  "license": "CC-BY-NC-ND-4.0",
  "description": "Browser-based implementation of the mechanical turk portion of Vam.",
  "main": "source/index.js",
  "scripts": {
    "clean": "rm -rf build/",
    "clean:full": "rm -rf build/ coverage/ node_modules/ yarn.lock",
    "build": "./node_modules/.bin/webpack",
    "build:production": "NODE_ENV=production ./node_modules/.bin/webpack",
    "watch": "yarn build -- --watch",
    "rebuild": "npm run clean && npm run build",
    "lint:javascript": "./node_modules/.bin/eslint ./",
    "lint:stylus": "./node_modules/.bin/stylint ./source/",
    "lint:fix": "./node_modules/.bin/eslint ./ --fix",
    "lint": "yarn run lint:fix && yarn run lint:stylus",
    "unit": "./node_modules/.bin/jest",
    "flow": "./node_modules/.bin/flow",
    "test": "yarn lint && yarn unit && yarn flow && yarn coverage:flow",
    "coverage": "./node_modules/.bin/codecov -t eb51943d-60b7-41f2-ba0c-4e3dc0ea802d",
    "coverage:flow": "./node_modules/.bin/flow-coverage-report",
    "start": "rm -rf build/*.js && (yarn watch & ./node_modules/.bin/webpack-dev-server)",
    "deploy": "yarn run clean && yarn run build:production && ./node_modules/.bin/gh-pages -d build",
    "precommit": "yarn lint:fix && yarn test"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./jestSetup.js"
    ],
    "transform": {
      ".": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage/",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/webpack.config.*.js**",
      "!**/flowTypes.js**",
      "!**/jestSetup.js**",
      "!**/index.js**",
      "!**/coverage/**",
      "!**/example/**",
      "!**/build/**",
      "!**/flow/**",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 97,
        "branches": 99,
        "functions": 96,
        "lines": 97
      }
    }
  },
  "flow-coverage-report": {
    "flowCommandPath": "./node_modules/.bin/flow",
    "outputDir": "./flow-coverage",
    "threshold": 85,
    "excludeGlob": [
      "./node_modules/**",
      "./source/**/*/",
      "./source/**/_*/**/*",
      "./**/*.test.js",
      "./**/*.styl",
      "./**/*.svg"
    ],
    "includeGlob": [
      "./source/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackiedrew/vam-online.git"
  },
  "keywords": [
    "react"
  ],
  "bugs": {
    "url": "https://github.com/mackiedrew/vam-online/issues"
  },
  "homepage": "https://valence.audio/",
  "dependencies": {
    "keyboardjs": "2.3.3",
    "promise-worker": "1.1.1",
    "promise-worker-transferable": "1.0.4",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "shortid": "2.2.8",
    "wav-decoder": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "codecov": "2.2.0",
    "css-loader": "0.28.2",
    "enzyme": "2.8.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-eslint-comments": "1.0.0",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsdoc": "3.1.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "2.1.1",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-security": "1.3.0",
    "eslint-plugin-unicorn": "2.1.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "0.11.1",
    "flow-bin": "0.46.0",
    "flow-coverage-report": "0.3.0",
    "gh-pages": "1.0.0",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "img-loader": "^2.0.0",
    "istanbul": "0.4.5",
    "jest": "20.0.4",
    "jest-cli": "20.0.1",
    "jest-css-modules": "1.1.0",
    "json-loader": "0.5.4",
    "offline-plugin": "4.8.1",
    "postcss-loader": "2.0.5",
    "postcss-svgo": "2.1.6",
    "prettier": "1.3.1",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "15.5.4",
    "redux-mock-store": "1.2.3",
    "sinon": "2.3.1",
    "style-loader": "0.18.1",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "svgo": "0.7.2",
    "webpack": "4.19.0",
    "webpack-dev-server": "2.4.5",
    "worker-loader": "0.8.0",
    "yarn": "0.24.5"
  }
}
