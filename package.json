{
  "name": "vam-server",
  "version": "0.0.1",
  "description": "Django server implementation of VAM.",
  "main": "index.js",
  "scripts": {
    "venv:install": "python3 -m venv ./virtual_environment/",
    "venv:start": "source ./virtual_environment/bin/activate",
    "pip": "pip3 install -r requirements.txt",
    "install": "npm run venv:install && npm run venv:start && npm run pip",
    "preinstall": "npm run install",
    "start:backend": "python3 backend/manage.py runserver",
    "start:frontend": "webpack-dev-server --hot --port 7447",
    "start": "npm run start:frontend",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackiedrew/vam-server.git"
  },
  "keywords": [
    "django",
    "graphene",
    "graphql",
    "python"
  ],
  "author": "Mackie Drew",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/mackiedrew/vam-server/issues"
  },
  "homepage": "https://valence.audio/",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.7.0",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
