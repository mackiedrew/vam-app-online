{
  "name": "vam-online",
  "version": "0.0.1",
  "description": "Django server and electron implementation of VAM.",
  "main": "index.js",
  "scripts": {
    "venv:install": "python3 -m venv ./virtual_environment/",
    "venv:start": "source ./virtual_environment/bin/activate",
    "pip": "pip3 install -r requirements.txt",
    "start:backend": "python3 backend/manage.py runserver",
    "start:frontend": "webpack-dev-server --hot --inline",
    "lint:frontend": "eslint ./",
    "lint": "npm run lint",
    "test": "jest",
    "start": "npm run start:frontend",
    "build": "webpack -p"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackiedrew/vam-online.git"
  },
  "keywords": [
    "django",
    "graphene",
    "graphql",
    "python"
  ],
  "author": "Mackie Drew",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/mackiedrew/vam-online/issues"
  },
  "homepage": "https://valence.audio/",
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "codecov": "^2.1.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "html-webpack-plugin": "2.28.0",
    "jest": "^19.0.2",
    "react-hot-loader": "3.0.0-beta.6",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2"
  }
}
