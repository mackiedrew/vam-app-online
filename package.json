{
  "name": "vam-online",
  "version": "0.0.1",
  "author": "Valence Audio <admin@valence.audio> (http://valence.audio/)",
  "contributors": [
    "Mackie Drew <mackiedrew@gmail.com> (http://mackiedrew.com/)"
  ],
  "license": "CC-BY-NC-ND-4.0",
  "description": "Electron implementation of VAM.",
  "main": "electron.js",
  "scripts": {
    "clean": "rm -rf build/",
    "clean:full": "rm -rf build/ coverage/ node_modules/",
    "build": "./node_modules/.bin/webpack",
    "build:production": "NODE_ENV=production ./node_modules/.bin/webpack",
    "watch": "yarn build -- --watch",
    "rebuild": "npm run clean && npm run build",
    "lint:javascript": "./node_modules/.bin/eslint ./",
    "lint:stylus": "./node_modules/.bin/stylint ./source/",
    "lint:fix": "yarn run lint --fix",
    "lint": "yarn run lint:javascript && yarn run lint:stylus",
    "test": "./node_modules/.bin/jest --config .jestrc.json",
    "coverage": "./node_modules/.bin/codecov",
    "start": " yarn watch & ./node_modules/.bin/webpack-dev-server",
    "deploy": "yarn run clean && yarn run build:production && ./node_modules/.bin/gh-pages -d build"
  },
  "jest": {
    "verbose": true,

    "setupFiles": [
      "./jestSetup.js"
    ],
    
    "transform": {
      ".": "<rootDir>/node_modules/babel-jest"
    },

    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],

    "moduleNameMapper": {
      ".+\\.(css|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },

    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    
    "collectCoverage": true,
    
    "coverageDirectory": "<rootDir>/coverage/",
    
    "collectCoverageFrom" : [
      "**/*.js",
      "!**/webpack.config.*.js**",
      "!**/jestSetup.js**",
      "!**/index.js**",
      "!**/coverage/**",
      "!**/example/**",
      "!**/source/workers/**",
      "!**/source/help/wav/**",
      "!**/build/**",
      "!**/node_modules/**"
    ],
    
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackiedrew/vam-online.git"
  },
  "keywords": [
    "react"
  ],
  "bugs": {
    "url": "https://github.com/mackiedrew/vam-online/issues"
  },
  "homepage": "https://valence.audio/",
  "dependencies": {
    "filter-obj": "1.1.0",
    "promise-worker": "1.1.1",
    "promise-worker-transferable": "1.0.4",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "shortid": "2.2.8",
    "wav-decoder": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "codecov": "2.1.0",
    "css-loader": "0.28.0",
    "enzyme": "2.8.1",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "0.11.1",
    "gh-pages": "0.12.0",
    "html-webpack-plugin": "2.28.0",
    "istanbul": "0.4.5",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "jest-css-modules": "1.1.0",
    "json-loader": "0.5.4",
    "postcss-loader": "1.3.3",
    "postcss-svgo": "2.1.6",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-svg-loader": "1.1.1",
    "react-test-renderer": "15.5.4",
    "serviceworker-webpack-plugin": "0.2.1",
    "sinon": "2.1.0",
    "style-loader": "0.16.1",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "svgo": "0.7.2",
    "web-audio-mock-api": "1.0.0",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "worker-loader": "0.8.0",
    "yarn": "0.22.0"
  }
}
