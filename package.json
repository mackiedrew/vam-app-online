{
  "name": "vam-online",
  "version": "0.0.1",
  "author": "Valence Audio <admin@valence.audio> (http://valence.audio/)",
  "contributors": [
    "Mackie Drew <mackiedrew@gmail.com> (http://mackiedrew.com/)"
  ],
  "license": "CC-BY-NC-ND-4.0",
  "description": "Electron implementation of VAM.",
  "main": "electron.js",
  "scripts": {
    "watch": "./node_modules/.bin/webpack --watch",
    "clean": "rm -rf ./build/",
    "build": "./node_modules/.bin/webpack -p",
    "rebuild": "npm run clean && npm run build",
    "lint": "./node_modules/.bin/eslint ./",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/codecov -t 08a9c11b-ea34-46f8-a03c-90802a1528e9",
    "electron": "./node_modules/.bin/electron .",
    "start": "npm run rebuild && (npm run electron & npm run watch)",
    "deploy": "./node_modules/.bin/electron-packager . VAM --all --overwrite --out binaries/"
  },
  "jest": {
    "transform": {
      ".": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/binaries/",
      "/coverage/",
      "/build/",
      "/node_modules/",
      "/jestsetup.js",
      "(.test)\\.(ts|tsx|js)$"
    ],
    "modulePathIgnorePatterns": [
      "/binaries/",
      "/coverage/",
      "/build/"
    ],
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "verbose": true,
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mackiedrew/vam-online.git"
  },
  "keywords": [
    "electron",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/mackiedrew/vam-online/issues"
  },
  "homepage": "https://valence.audio/",
  "dependencies": {
    "chart.js": "2.5.0",
    "electron": "1.6.2",
    "electron-reload": "1.1.0",
    "filter-obj": "1.1.0",
    "react": "15.4.2",
    "react-chartjs-2": "2.0.5",
    "react-dom": "15.4.2",
    "shortid": "2.2.8",
    "wav": "1.0.1"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "cheerio": "0.22.0",
    "codecov": "2.1.0",
    "css-loader": "0.28.0",
    "electron-packager": "8.6.0",
    "electron-rebuild": "1.5.7",
    "electron-webpack-plugin": "1.2.2",
    "enzyme": "2.8.0",
    "enzyme-to-json": "1.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "jest-css-modules": "1.1.0",
    "json-loader": "0.5.4",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-svg-loader": "1.1.1",
    "sinon": "2.1.0",
    "style-loader": "0.16.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "yarn": "0.21.3"
  }
}
